{
    "name": "vccn-discord-bot",
    "version": "1.0.1",
    "description": "Voice channel connections notification Discord bot",
    "main": "dist/bot.js",
    "scripts": {
        "start": "node dist/Main.js",
        "build": "npm run build-ts && npm run tslint",
        "build-ts": "tsc",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "nohup": "nohup node dist/Main.js > output.log &"
    },
    "author": "jsirgo",
    "repository": {
        "type": "git",
        "url": "https://github.com/jsirgo/vccn-discord-bot.git"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "discord.js": "11.5.1",
        "string-template": "1.0.0"
    },
    "devDependencies": {
        "@types/node": "12.11.1",
        "@types/string-template": "1.0.2",
        "tslint": "5.20.0",
        "tslint-eslint-rules": "5.4.0",
        "typescript": "3.6.4"
    }
}
